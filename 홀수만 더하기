T = int(input())

for test_case in range(1, T + 1):
    array = list(map(int, input().split(" ")))
    sum=0
     
    for i in range(0, len(array)):
        if(array[i]<0 or array[i]>10000):
            continue
         
        if(array[i]%2==1):
            sum+=array[i]
        
    print("#{} {}".format(test_case, sum))
